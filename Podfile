source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_frameworks!
inhibit_all_warnings!

target ‘ZhihuDailyReport’ do
    pod 'Alamofire'
    pod 'Kingfisher’
    pod 'SnapKit'
    pod 'PKHUD'
    pod 'FSPagerView'
    pod 'UzysAnimatedGifPullToRefresh'
    pod 'SwiftyJSON'
end
